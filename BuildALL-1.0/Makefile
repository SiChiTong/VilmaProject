install: libvilma_ros_plugin.so
	install my_world.world $(HOME)
	install my_world_crossroad.world $(HOME)
	install my_world_crossroad2.world $(HOME)
	if [ ! -d ~/.gazebo/models ] ;then mkdir -p ~/.gazebo/models; fi
	cp -r vilma_draw ~/.gazebo/models
	cp -r vilma_vehicle ~/.gazebo/models
	cp -r environments/. ~/.gazebo/models	
	install libvilma_ros_plugin.so /usr/lib/gazebo-2.1/plugins
	sudo chown $(SUDO_USER) /usr/lib/gazebo-2.1/plugins/libVilma2Plugin.so
	sudo chown $(SUDO_USER) /usr/lib/gazebo-2.1/plugins/libvilma_ros_plugin.so
.PHONY: install
